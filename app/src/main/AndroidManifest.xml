<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.recyclerPresident">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.recyclerPresident.Activity.DisplaySpinnerActivity"></activity>
        <activity android:name="com.example.recyclerPresident.Activity.FormActivity" />
        <activity android:name="com.example.recyclerPresident.Activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.recyclerPresident.Activity.PresidentDetailsActivity"
            android:parentActivityName="com.example.recyclerPresident.MainActivity" />
        <activity android:name="com.example.recyclerPresident.MainActivity"></activity>
    </application>

</manifest>